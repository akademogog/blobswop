.container.login__container
    .row
        h1.login__title Mit Vollgas weiter…
    .row.justify-content-md-center
        form(action="#" method="POST").col-lg-5.col-md-5.login__form
            .input-block
                input(type="text" placeholder="E-Mail" data-require="true" data-valid="email")
                .message E-Mail eingeben
            .input-block
                input(type="password" placeholder="Passwort" data-require="true")
                .message passwort eingeben
            .form__row.d-flex.justify-content-between
                a(href="#") Passwort vergessen?
                button(type="submit").btn Let’s go!  
        .col-lg-2.col-md-1.login__oder
            .line
            p oder
            .line
